{
    "name": "mobi-market/black-belt",
    "description": "BlackBelt API integration",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.3",
        "illuminate/support": ">=5.8 <10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Scott-David Jones",
            "email": "scott@mobi-market.co.uk"
        },
        {
            "name": "Callum Slaney-Flett",
            "email": "callum@mobi-market.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "MobiMarket\\BlackBelt\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MobiMarket\\BlackBelt\\BlackBeltServiceProvider"
            ],
            "aliases": {
                "BlackBelt": "MobiMarket\\BlackBelt\\BlackBeltFacade"
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
